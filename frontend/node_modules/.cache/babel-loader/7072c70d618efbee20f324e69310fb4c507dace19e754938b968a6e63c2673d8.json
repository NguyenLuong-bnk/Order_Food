{"ast":null,"code":"import \"core-js/modules/es.object.define-property.js\";\nimport { ref } from 'vue';\nimport MenuItem from './MenuItem.vue';\nimport menuItems from './menuItems';\n\n// booleano para contraer o expandir menú\n\nexport default {\n  __name: 'MenuTemplate',\n  setup: function setup(__props, _ref) {\n    var expose = _ref.expose;\n    expose();\n    var collapsed = ref(false);\n    var __returned__ = {\n      get collapsed() {\n        return collapsed;\n      },\n      set collapsed(v) {\n        collapsed = v;\n      },\n      ref: ref,\n      MenuItem: MenuItem,\n      get menuItems() {\n        return menuItems;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","MenuItem","menuItems","collapsed"],"sources":["C:/Users/luong/Downloads/Order/frontend/src/admin/components/MenuTemplate.vue"],"sourcesContent":["<script setup>\r\n\r\nimport { ref } from 'vue';\r\nimport MenuItem from './MenuItem.vue';\r\nimport menuItems from './menuItems';\r\n\r\n\r\n// booleano para contraer o expandir menú\r\nlet collapsed = ref(false);\r\n\r\n</script>\r\n\r\n<template>\r\n\r\n<!-- clase menu, y si es verdadero collapsed va a aplicar clase collapsed, sino expanded -->\r\n<aside :class=\"['menu', collapsed ? 'collapsed' : 'expanded']\">\r\n\r\n    <div class=\"header\">\r\n        <button class=\"menu-button\" @click.prevent=\"collapsed = !collapsed\">\r\n            <font-awesome-icon icon=\"fa-solid fa-bars\" size=\"2x\"/>\r\n        </button>\r\n    </div>\r\n\r\n    <div class=\"profile\" v-if=\"!collapsed\">\r\n        <img class=\"profile__img\" src=\"../assets/profile.svg\" :alt=menuItems.profile.img>\r\n        <!-- cuando este expandido (no collapsed) se va a mostrar name -->\r\n        <div class=\"profile__name\"> <a href=\"#\" @click.prevent=\"menuItems.profile.onClick\"> {{ menuItems.profile.name }} </a> </div>\r\n    </div>\r\n\r\n    <div class=\"menu__item\" >\r\n\r\n        <ul>\r\n            <MenuItem v-for=\"item in menuItems.items\" :key=\"item.id\" :item=\"item\" :collapsed=\"collapsed\" />\r\n        </ul>\r\n\r\n    </div>\r\n\r\n</aside>\r\n\r\n\r\n\r\n</template>\r\n\r\n<style scoped>\r\n\r\n.menu {\r\n    background-color: var(--colorNegro);\r\n    height: 100vh;\r\n    transition: width .3s;\r\n    position: fixed;\r\n    left: 0;\r\n    top: 0;\r\n}\r\n\r\n.collapsed {\r\n    width: 8rem;\r\n    /* height: 4.4rem; */\r\n}\r\n\r\n.expanded {\r\n    width: 35rem;\r\n}\r\n\r\n.header {\r\n    margin: 0 auto;\r\n    display: flex;\r\n    justify-content: end;\r\n}\r\n\r\n.menu-button {\r\n    width: 4.4rem;\r\n    height: 4.4rem;\r\n    border: none;\r\n    background-color: transparent;\r\n    margin: 0 auto;\r\n    display: block;\r\n}\r\n\r\n.profile {\r\n    display: flex;\r\n    padding: var(--separacion);\r\n    gap: var(--separacion);\r\n    align-items: center;\r\n}\r\n\r\n.profile__img {\r\n    width: 4.4rem;\r\n}\r\n\r\n.profile__name {\r\n    text-align: center;\r\n}\r\n\r\n.profile__name a {\r\n    text-decoration: none;\r\n    text-transform: capitalize;\r\n}\r\n\r\n.menu__item {\r\n\r\n}\r\n\r\n.menu__item ul {\r\n    padding: 0;\r\n    list-style: none;\r\n    margin: 0;\r\n}\r\n\r\n</style>"],"mappings":";AAEA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,QAAQ,MAAM,gBAAgB;AACrC,OAAOC,SAAS,MAAM,aAAa;;AAGnC;;;;;;;IACA,IAAIC,SAAS,GAAGH,GAAG,CAAC,KAAK,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}